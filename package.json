{
  "name": "cli-command",
  "version": "0.8.6",
  "description": "Command execution for cli programs",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/cli-command"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/cli-command/issues",
    "email": "muji <noop@xpm.io>"
  },
  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/freeformsystems/cli-command/blob/master/LICENSE"
  }],
  "keywords": [
    "cli",
    "parse",
    "option",
    "argument",
    "command"
  ],
  "dependencies": {
    "cli-define": "~0.4.12",
    "cli-native": "~1.0.0",
    "cli-env": "~1.0.3",
    "cli-argparse": "~0.3.6",
    "cli-error": "~0.5.0",
    "cli-logger": "~0.5.4",
    "cli-util": "~1.1.7",
    "cli-fs": "~1.0.2",
    "cli-rc": "~1.0.0"
  },
  "devDependencies": {
    "ttycolor": "~0.8.4",
    "mocha": "~1.17.0",
    "chai": "~1.9.0",
    "istanbul": "~0.2.4"
  },
  "engine": [ "node >= 0.10.21" ],
  "scripts": {
    "clean": "rm -f ./log/*.log",
    "help2man": "CLI_TOOLKIT_HELP2MAN=1 help2man --no-info --output=./.man/defaults.1 ./bin/help/defaults && man ./.man/defaults.1",
    "help2json": "CLI_TOOLKIT_HELP_JSON=1 CLI_TOOLKIT_HELP_JSON_INDENT=2 ./bin/help/defaults",
    "permrm": "chmod -r test/unit/rc/eaccess.json",
    "permadd": "chmod +r test/unit/rc/eaccess.json",
    "test": "npm run clean; npm run permrm; istanbul cover _mocha -- -u bdd --recursive --bail --reporter list -A test/unit/; npm run permadd"
  },
  "config" : {}
}
