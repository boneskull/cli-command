#!/usr/bin/env node

/**
 *  Example program to test executing subcommands.
 *
 *  ./bin/exec -h
 *  ./bin/exec --version
 *  ./bin/exec init
 */
var path = require('path');
require('ttycolor')().defaults();
var pkg = path.normalize(path.join(__dirname, '..', 'package.json'));
var cli = require('..')(pkg);
cli
  .version()
  .help()
  .command('init', 'execute exec-init')
  .parse();
